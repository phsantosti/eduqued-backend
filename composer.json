    "coffeecode/datalayer": "2.0.7",
    "monolog/monolog": "3.7.0",
    "claudsonm/cep-promise-php": "v1.2.0",
    "dompdf/dompdf": "v3.0.0",
    "chillerlan/php-qrcode": "4.2.0",
    "firebase/php-jwt": "v6.1.0",
